@model IEnumerable<mungmunglogServer.Models.FamilyMember>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IsMaster)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Family)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IsMaster)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Family.FamilyId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FamilyMemberId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FamilyMemberId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FamilyMemberId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
